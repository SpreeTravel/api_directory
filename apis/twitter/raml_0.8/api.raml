      responses: 
        200: 
          body: 
            application/json: 
              schema: lists
              example: !include examples/lists-subscribition-example.json
  /ownerships{mediaTypeExtension}: 
    type: base
    get: 
      is: [ acceptsUser , limitableByCount: { max: 1000 } ]
      description: |
